{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "10.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "panels": [],
      "title": "Environment Summary Statistics",
      "type": "row"
    },
    {
      "id": 1,
      "title": "Total Topics",
      "type": "stat",
      "gridPos": { "h": 5, "w": 4, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "fh_prom_ds" },
      "targets": [
        {
          "expr": "sum(kpow_topic_count{env=~\"$env\", target=\"all\"})",
          "instant": true,
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": { "color": { "mode": "fixed", "fixedColor": "blue" } }
      }
    },
    {
      "id": 2,
      "title": "Total Replica Disk",
      "type": "stat",
      "gridPos": { "h": 5, "w": 4, "x": 4, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "fh_prom_ds" },
      "targets": [
        {
          "expr": "sum(kpow_topic_bytes_disk_total{env=~\"$env\", target=\"all\"})",
          "instant": true,
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": { "mode": "fixed", "fixedColor": "blue" }
        }
      }
    },
    {
      "id": 3,
      "title": "Write/s (msgs)",
      "type": "stat",
      "gridPos": { "h": 5, "w": 4, "x": 8, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "fh_prom_ds" },
      "targets": [
        {
          "expr": "sum(kpow_topic_end_delta_total{env=~\"$env\", target=\"all\"})",
          "instant": true,
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "fixed", "fixedColor": "green" }
        }
      }
    },
    {
      "id": 4,
      "title": "Read/s (msgs)",
      "type": "stat",
      "gridPos": { "h": 5, "w": 4, "x": 12, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "fh_prom_ds" },
      "targets": [
        {
          "expr": "sum(kpow_group_offset_delta_total{env=~\"$env\", target=\"all\"})",
          "instant": true,
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "fixed", "fixedColor": "green" }
        }
      }
    },
    {
      "id": 5,
      "title": "Non-Preferred Leaders",
      "type": "stat",
      "gridPos": { "h": 5, "w": 4, "x": 16, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "fh_prom_ds" },
      "targets": [
        {
          "expr": "sum(kpow_topic_non_preferred_leaders_total{env=~\"$env\", target=\"all\"})",
          "instant": true,
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 0.1 }
            ]
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Total URPs",
      "type": "stat",
      "gridPos": { "h": 5, "w": 4, "x": 20, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "fh_prom_ds" },
      "targets": [
        {
          "expr": "sum(kpow_topic_urp_total{env=~\"$env\", target=\"all\"})",
          "instant": true,
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0.1 }
            ]
          }
        }
      }
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 6 },
      "id": 101,
      "panels": [],
      "title": "Topic Production & Consumption Throughput",
      "type": "row"
    },
    {
      "id": 7,
      "title": "Topic Production Throughput (m/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 7 },
      "datasource": { "type": "prometheus", "uid": "fh_prom_ds" },
      "targets": [
        {
          "expr": "kpow_topic_end_delta{env=~\"$env\", target=~\"$topic_name\", quantile=\"0.95\"}",
          "legendFormat": "{{target}}",
          "refId": "A"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "id": 11,
      "title": "Topic Consumption Throughput (m/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 7 },
      "datasource": { "type": "prometheus", "uid": "fh_prom_ds" },
      "targets": [
        {
          "expr": "kpow_topic_offset_delta{env=~\"$env\", target=~\"$topic_name\", quantile=\"0.95\"}",
          "legendFormat": "{{target}}",
          "refId": "A"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 15 },
      "id": 103,
      "panels": [],
      "title": "Topic Size & Lag",
      "type": "row"
    },
    {
      "id": 8,
      "title": "Topic Size Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "fh_prom_ds" },
      "targets": [
        {
          "expr": "kpow_topic_bytes_disk{env=~\"$env\", target=~\"$topic_name\"}",
          "legendFormat": "{{target}}",
          "refId": "A"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "fieldConfig": { "defaults": { "unit": "bytes" } }
    },
    {
      "id": 9,
      "title": "Topic Consumer Lag",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "fh_prom_ds" },
      "targets": [
        {
          "expr": "kpow_topic_offset_lag{env=~\"$env\", target=~\"$topic_name\", quantile=\"0.95\"}",
          "legendFormat": "{{target}}",
          "refId": "A"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 24 },
      "id": 102,
      "panels": [],
      "title": "URP Diagnostics",
      "type": "row"
    },
    {
      "id": 10,
      "title": "URPs by Topic",
      "type": "table",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 25 },
      "datasource": { "type": "prometheus", "uid": "fh_prom_ds" },
      "targets": [
        {
          "expr": "kpow_topic_urp{env=~\"$env\", target=~\"$topic_name\"}",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "options": {
        "sortBy": [
          { "displayName": "Environment", "desc": false },
          { "displayName": "Topic Name", "desc": false }
        ]
      },
      "fieldConfig": {
        "defaults": { "custom": { "align": "auto", "displayMode": "auto" } }
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "domain": true,
              "id": true,
              "instance": true,
              "job": true
            },
            "indexByName": {
              "Value": 2,
              "env": 0,
              "target": 1
            },
            "renameByName": {
              "Value": "URPs",
              "env": "Environment",
              "target": "Topic Name"
            }
          }
        }
      ]
    }
  ],
  "refresh": "1m",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["kafka", "kpow", "topic"],
  "templating": {
    "list": [
      {
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "datasource": { "type": "prometheus", "uid": "fh_prom_ds" },
        "definition": "label_values(kpow_topic_count, env)",
        "hide": 0,
        "includeAll": true,
        "label": "Environment",
        "multi": true,
        "name": "env",
        "options": [],
        "query": "label_values(kpow_topic_count, env)",
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      },
      {
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "datasource": { "type": "prometheus", "uid": "fh_prom_ds" },
        "definition": "label_values(kpow_topic_bytes_disk{env=~\"$env\"}, target)",
        "hide": 0,
        "includeAll": true,
        "label": "Topic",
        "multi": true,
        "name": "topic_name",
        "options": [],
        "query": "label_values(kpow_topic_bytes_disk{env=~\"$env\"}, target)",
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Kafka Topic",
  "uid": "kafka-topic",
  "version": 1,
  "weekStart": ""
}
